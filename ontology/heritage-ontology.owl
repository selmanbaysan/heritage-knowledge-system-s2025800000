<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/selmanbaysan/ontologies/2025/11/heritage-ontology#"
     xml:base="http://www.semanticweb.org/selmanbaysan/ontologies/2025/11/heritage-ontology"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/selmanbaysan/ontologies/2025/11/heritage-ontology"/>
    

    <owl:ObjectProperty rdf:about="#approvedBy">
        <owl:inverseOf rdf:resource="#approves"/>
        <rdfs:domain rdf:resource="#Person"/>
        <rdfs:range rdf:resource="#TribalElder"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="#approves">
        <rdfs:domain rdf:resource="#TribalElder"/>
        <rdfs:range rdf:resource="#Person"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#caresFor">
        <rdfs:domain rdf:resource="#Person"/>
        <rdfs:range rdf:resource="#CulturalItem"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#hasPerformer">
        <rdfs:domain rdf:resource="#Recording"/>
        <rdfs:range rdf:resource="#Performer"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#hasRole">
        <owl:inverseOf rdf:resource="#isRoleOf"/>
        <rdfs:domain rdf:resource="#Person"/>
        <rdfs:range rdf:resource="#Role"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#isRoleOf">
        <rdfs:domain rdf:resource="#Role"/>
        <rdfs:range rdf:resource="#Person"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#locatedAt">
        <rdfs:domain rdf:resource="#CulturalItem"/>
        <rdfs:range rdf:resource="#Location"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#performedBy">
        <owl:inverseOf rdf:resource="#performs"/>   
        <rdfs:domain rdf:resource="#Performance"/>
        <rdfs:range rdf:resource="#Performer"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#performs">
        <rdfs:domain rdf:resource="#Performer"/>
        <rdfs:range rdf:resource="#Performance"/>
    </owl:ObjectProperty>


    <owl:DatatypeProperty rdf:about="#performerAccessLevel">
        <rdfs:domain rdf:resource="#Person"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment>The access level granted to a performer (e.g., 1=Public, 5=Sacred)</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#humanApproval">
        <rdfs:domain rdf:resource="#Person"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment>Explicit boolean flag indicating if a human has validated this person.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#recordingDate">
        <rdfs:domain rdf:resource="#Recording"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#requiredAccessLevel">
        <rdfs:domain rdf:resource="#RestrictedItem"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#restrictionEffectiveDate">
        <rdfs:domain rdf:resource="#RestrictedItem"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
    </owl:DatatypeProperty>

    <owl:Class rdf:about="#CulturalEntity"/>

    <owl:Class rdf:about="#CulturalItem">
        <rdfs:subClassOf rdf:resource="#CulturalEntity"/>
    </owl:Class>

    <owl:Class rdf:about="#Location">
        <rdfs:subClassOf rdf:resource="#CulturalEntity"/>
    </owl:Class>

    <owl:Class rdf:about="#SacredSite">
        <rdfs:subClassOf rdf:resource="#Location"/>
    </owl:Class>

    <owl:Class rdf:about="#Person">
        <rdfs:subClassOf rdf:resource="#CulturalEntity"/>
    </owl:Class>

    <owl:Class rdf:about="#Archaeologist">
        <rdfs:subClassOf rdf:resource="#Person"/>
    </owl:Class>

    <owl:Class rdf:about="#Performer">
        <rdfs:subClassOf rdf:resource="#Person"/>
    </owl:Class>

    <owl:Class rdf:about="#Role">
        <rdfs:subClassOf rdf:resource="#CulturalEntity"/>
    </owl:Class>

    <owl:Class rdf:about="#GuardianRole">
        <rdfs:subClassOf rdf:resource="#Role"/>
        <rdfs:comment>The 'ticket' or 'role object' assigned to a person.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="#TribalElder">
        <rdfs:subClassOf rdf:resource="#Role"/>
    </owl:Class>

    <owl:Class rdf:about="#ResearcherRole">
        <rdfs:subClassOf rdf:resource="#Role"/>
        <rdfs:comment>Role assigned to archaeologists or researchers.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="#SpiritualGuardian">
        <rdfs:subClassOf rdf:resource="#Role"/>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#Person"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasRole"/>
                        <owl:someValuesFrom rdf:resource="#GuardianRole"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#caresFor"/>
                        <owl:someValuesFrom rdf:resource="#SacredItem"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment>Defined Class: Any Person who has a GuardianRole AND cares for a SacredItem.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="#MusicalInstrument">
        <rdfs:subClassOf rdf:resource="#CulturalItem"/>
    </owl:Class>

    <owl:Class rdf:about="#Artifact">
        <rdfs:subClassOf rdf:resource="#CulturalItem"/>
        <rdfs:comment>Physical objects of cultural significance like coins, pottery, etc.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="#SacredItem">
        <rdfs:subClassOf rdf:resource="#CulturalItem"/>
    </owl:Class>

    <owl:Class rdf:about="#RestrictedItem">
        <rdfs:subClassOf rdf:resource="#CulturalItem"/>
        <rdfs:comment>Items that have temporal or clearance restrictions.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="#Ritual">
        <rdfs:subClassOf rdf:resource="#CulturalItem"/>
    </owl:Class>

    <owl:Class rdf:about="#Recording">
        <rdfs:subClassOf rdf:resource="#CulturalItem"/>
        <rdfs:comment>A recording of a performance, subject to temporal access validation.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="#Event">
        <rdfs:subClassOf rdf:resource="#CulturalEntity"/>
    </owl:Class>

    <owl:Class rdf:about="#Performance">
        <rdfs:subClassOf rdf:resource="#Event"/>
    </owl:Class>

</rdf:RDF>